                          <a href="#"><i class="icon-thumbs-down"></i>Unhelpful (0)</a>
                        </div><!-- End .review-action -->
                      </div><!-- End .col-auto -->
                    </div><!-- End .row -->
                  </div><!-- End .review -->
                </div><!-- End .reviews -->
              </div><!-- .End .tab-pane -->
