          <div class="product-details-top">
            <div class="row">
              <div class="col-md-6">
                <div class="product-gallery product-gallery-vertical">
                  <div class="row">
                    <figure class="product-main-image">
                      <img id="product-zoom" src="/Products/<%= allproduct.image[1] %>" data-zoom-image="/Products/<%= allproduct.image[1] %>" alt="product image">

                      <a href="#" id="btn-product-gallery" class="btn-product-gallery">
                        <i class="icon-arrows"></i>
                      </a>
                    </figure><!-- End .product-main-image -->

                    <div id="product-zoom-gallery" class="product-image-gallery">
                      <a class="product-gallery-item active" href="#" data-image="/Products/<%= allproduct.image[1] %>" data-zoom-image="/Products/<%= allproduct.image[1] %>">
                        <img src="/Products/<%= allproduct.image[1] %>" alt="product side">
                      </a>

                      <a class="product-gallery-item" href="#" data-image="/Products/<%= allproduct.image[0] %>" data-zoom-image="/Products/<%= allproduct.image[0] %>">
                        <img src="/Products/<%= allproduct.image[0] %>" alt="product cross">
                      </a>

                      <a class="product-gallery-item" href="#" data-image="/Products/<%= allproduct.image[0] %>" data-zoom-image="/Products/<%= allproduct.image[0] %>">
                        <img src="/Products/<%= allproduct.image[0] %>" alt="product with model">
                      </a>

                      <a class="product-gallery-item" href="#" data-image="/Products/<%= allproduct.image[0] %>" data-zoom-image="/Products/<%= allproduct.image[0] %>">
                        <img src="/Products/<%= allproduct.image[0] %>" alt="product back">
                      </a>
                    </div><!-- End .product-image-gallery -->
                  </div><!-- End .row -->
                </div><!-- End .product-gallery -->
              </div><!-- End .col-md-6 -->

              <div class="col-md-6">
                <div class="product-details">
                  <h1 class="product-title"><%= allproduct.productname %> </h1><!-- End .product-title -->

                  <div class="ratings-container">
                    <div class="ratings">
                      <div class="ratings-val" style="width: 80%;"></div><!-- End .ratings-val -->
                    </div><!-- End .ratings -->
                    <a class="ratings-text" href="#product-review-link" id="review-link">( 2 Reviews )</a>
                  </div><!-- End .rating-container -->

                  <div class="product-price d-flex flex-column justify-content-start align-items-start">
                   <p><del>₹<%= allproduct. price %> </del></p> 
                    ₹<%=Math.floor((allproduct. price-(allproduct. price*allproduct.offer/100))).toFixed(2)%> 
                   
                       <p>you saved <%= allproduct.offer%>%</p> 
                  
                  </div><!-- End .product-price -->

                  <div class="product-content" style="color: black;">
                    <ul style="color: hwb(0 0% 100%);">
                      <li>●<%=allproduct.ram  %> | <%=allproduct.internal  %></li>
                      <li>●<%=allproduct.size  %> </li>
                      <li>●<%=allproduct.os  %> </li>
                      <li>●<%=allproduct.mah  %> </li>
                      <li>●<%=allproduct.watt  %> </li>
                      <li>●<%=allproduct.colors %></li>
                    </ul>
                  </div><!-- End .product-content -->
